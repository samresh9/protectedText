{
  "name": "protectedtext",
  "private": true,
  "packageManager": "npm@9.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18.16.0 <19",
    "npm": ">=9.5.0 <10"
  },
  "scripts": {
    "start:backend": "turbo run start --filter=backend",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "prettier": "^2.5.1",
    "turbo": "latest"
  }
}
